{
  "name": "regear",
  "type": "module",
  "version": "0.0.2",
  "description": "Web App for Re:Gear Dunedin",
  "repository": "https://github.com/jayde-medder/regear",
  "author": "Jayde Medder",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "run-p dev:client dev:server",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "run-s build:client build:server",
    "build:client": "vite build",
    "build:server": "tsc --project tsconfig.server.json && esbuild --packages=external --platform=node --format=esm --outfile=dist/server.js --bundle server/index.ts",
    "test": "vitest",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier -w '**/*.{js,jsx,ts,tsx}'"
  },
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.3",
    "@devnomic/marquee": "^1.0.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.17.2",
    "express-jwt": "^8.4.1",
    "express-jwt-authz": "^2.4.1",
    "jwks-rsa": "^3.1.0",
    "knex": "^3.0.1",
    "lucide-react": "^0.366.0",
    "sqlite3": "^5.1.6",
    "superagent": "7.1.1",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.0",
    "vaul": "^0.9.0"
  },
  "devDependencies": {
    "@auth0/auth0-react": "^1.12.0",
    "@devacademy/eslint-config": "^1.9.1",
    "@testing-library/react": "^14.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^18.19.31",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/superagent": "^4.1.18",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-c8": "^0.30.1",
    "autoprefixer": "^10.4.19",
    "esbuild": "^0.18.19",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "2.6.1",
    "react": "^18.0.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "superagent": "7.1.1",
    "tailwindcss": "^3.4.3",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "browserslist": "> 2%, not dead"
}
